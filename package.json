{
  "name": "mydealership",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "npm run lint && next build",
    "update-types": "npx supabase gen types typescript --project-id $(grep NEXT_PUBLIC_SUPABASE_PROJECT_ID .env.local | cut -d '=' -f 2 | tr -d '[:space:]') > types/supabase.ts",
    "update": "git pull && npm install && npm run update-types",
    "lint": "eslint '*/**/*.{js,jsx,ts,tsx}' --quiet",
    "lint:fix": "eslint '*/**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "immer": "^10.0.3",
    "lucide-react": "^0.292.0",
    "next": "14.1.0",
    "next-intl": "^3.5.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.50.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "daisyui": "^4.6.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.1",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "supabase": "^1.123.0",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
